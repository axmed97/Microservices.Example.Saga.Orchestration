    <UserSecretsId>dotnet-SagaStateMachine.Service-fe29c553-ce29-4c2e-bd23-6a36b9e3a1b9</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MassTransit.EntityFrameworkCore" Version="8.2.4" />
